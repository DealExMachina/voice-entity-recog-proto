{
  "name": "mastra-voice-entity-extraction",
  "version": "1.0.0",
  "description": "Mastra prototype for entity extraction from voice chat with DuckDB storage via MCP",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:ts": "npx tsx src/index-ts.ts",
    "dev": "node --watch src/index.js",
    "dev:ts": "npx tsx --watch src/index-ts.ts",
    "build": "npx tsc",
    "build:check": "npx tsc --noEmit",
    "test": "node tests/test-basic.js",
    "test:ts": "npx tsx tests/test-typescript.ts",
    "test:flow": "npx tsx tests/test-entity-flow.ts",
    "test:watch": "node --watch tests/test-basic.js",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "setup": "./scripts/setup-keys.sh",
    "config": "./scripts/check-config.sh",
    "deploy": "./scripts/deploy-koyeb.sh",
    "deploy:check": "echo 'Deploy via GitHub Actions or Koyeb CLI'",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "logs": "echo 'Use: docker logs <container> or check deployment platform'"
  },
  "keywords": [
    "mastra",
    "voice",
    "entity-extraction",
    "mcp",
    "duckdb",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@duckdb/node-api": "^1.3.1-alpha.22",
    "@mistralai/mistralai": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.1",
    "nodemon": "^3.0.1"
  }
}
